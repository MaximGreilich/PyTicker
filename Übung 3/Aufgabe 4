from datetime import datetime, timedelta

class DatetimeModul:
    now = datetime.now()
    time = timedelta(days=90, hours= 3, minutes= 30)
    deadline = now + time

    print(deadline)
    heiligabend= datetime(year=2026, month=12, day=24, hour=18)
    print("Heiligabend 2026 ist ein:", heiligabend.strftime("%A"))

