#grma1075 94731
class EineKlasse:
    x= 5
    def __init__(self,title, val):
        self.title = title
        self.val = val
    def __str__(self):
        return f"Title:{self.title}, Wert:{self.val}, x:{self.x}"
    
p1 = EineKlasse("Objekt1", 10)
p2 = EineKlasse("Objekt2", 20)
p3 = EineKlasse("Objekt3", 30)

print(p1, p2, p3)

#Hier wird das Klassenattribut komplett überschrieben, also wird der x-wert bei jedem Objekt 20
EineKlasse.x = 20 
print(p1, p2, p3)

#Hier werden die Klassenattribute von Instanz-Attributen überschrieben, daher ist es egal was das Klassenattribut für einen Wert hat
p1.x=5
p2.x=5
p3.x=5
print(p1, p2, p3)
    
    
    