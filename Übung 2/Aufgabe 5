class IntSet: 
    def __init__(self, initial_list):
        self.elements= list(set(initial_list))

    def __add__(self, other):

        combined_list = self.elements + other.elements
        return IntSet(combined_list)
    def __str__(self):
        string_elements = [str(e)for e in self.elements]
        inside_string = ",".join(string_elements)

        return f"{inside_string}"
    
set_a = IntSet([1,5,10])
set_b = IntSet([10,20,30])

print(f"Set A: {set_a}")
print(f"Set B: {set_b}")

set_c = set_a + set_b

print(f"Set C: {set_c}")

#musste mir bei dieser Aufgabe ein bisschen von AI helfen lassen, da ich Probleme innerhalb meiner __str__-funktion hatte.